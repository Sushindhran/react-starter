{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React + React Router + Webpack 2 + Mocha + Chai",
  "main": "dist/index.html",
  "engines": {
    "npm": "3.10.3",
    "node": "6.3.0"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config src/config/webpack/webpack.config.js --inline --progress --history-api-fallback",
    "lint": "eslint --config src/config/eslint/eslint.config.json src/js/**",
    "lint:test": "eslint --config src/config/eslint/eslint.config.json --env mocha test/js/**",
    "setup": "node src/config/prompts/setup.js",
    "test": "mocha test/testSetup.js test/**/*.spec.js --reporter progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neosavvyinc/react-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neosavvyinc/react-starter/issues"
  },
  "homepage": "https://github.com/neosavvyinc/react-starter#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "bluebird": "3.4.1",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.2.0",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-mock": "5.0.5",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.14.1",
    "mobx": "2.4.1",
    "mobx-provide": "1.0.3",
    "mobx-react": "3.5.3",
    "mobx-react-devtools": "4.2.4",
    "mocha": "3.0.0",
    "node-sass": "3.8.0",
    "prompt": "1.0.0",
    "react": "15.3.0",
    "react-addons-test-utils": "15.3.0",
    "react-dom": "15.3.0",
    "react-router": "2.6.0",
    "sass-loader": "4.0.0",
    "sinon": "1.17.5",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {}
}
